shasum":"195c5c2f122db2f79d9863babc320cd4e607d887","_from":".","_npmVersion":"1.4.21","_npmUser":{"name":"kapouer","email":"kapouer@melix.org"},"maintainers":[{"name":"kapouer","email":"kapouer@melix.org"}],"dist":{"shasum":"195c5c2f122db2f79d9863babc320cd4e607d887","tarball":"https://registry.npmjs.org/marked-man/-/marked-man-0.1.4.tgz"},"directories":{}},"0.1.5":{"name":"marked-man","version":"0.1.5","description":"wrapper adding manpage output to 'marked', inspired by 'ronn'","main":"lib/marked-man.js","bin":{"marked-man":"./bin/marked-man"},"man":["./man/marked-man.1"],"scripts":{"test":"node ./test/compare"},"repository":{"type":"git","url":"git://github.com/kapouer/marked-man.git"},"keywords":["markdown","roff","ronn"],"author":{"name":"Jérémy Lal","email":"kapouer@melix.org"},"license":"MIT","dependencies":{"marked":"^0.3.2"},"gitHead":"5aea4dc5a9ef173ebfd007e38bb2975b15c734b6","bugs":{"url":"https://github.com/kapouer/marked-man/issues"},"_id":"marked-man@0.1.5","_shasum":"797f50c2565ff0f0b90803c6092673ac91e55e39","_from":".","_npmVersion":"1.4.21","_npmUser":{"name":"kapouer","email":"kapouer@melix.org"},"maintainers":[{"name":"kapouer","email":"kapouer@melix.org"}],"dist":{"shasum":"797f50c2565ff0f0b90803c6092673ac91e55e39","tarball":"https://registry.npmjs.org/marked-man/-/marked-man-0.1.5.tgz"},"directories":{}}},"readme":"marked-man(1) -- markdown to roff\n=================================\n\nSYNOPSIS\n--------\n\n```\nmarked-man README.md > doc/marked-man.1\n```\n\nSee [marked README](https://github.com/chjj/marked) for documentation about how to use marked.\n\nNote that `marked-man --format=html` is the same as `marked`.\n\n\nDESCRIPTION\n-----------\n\n`marked-man` wraps `marked` to extend it with groff output support.\n\n\nOPTIONS\n-------\n\n`marked-man` adds some options to `marked` existing options:\n\n* format\n  Sets the output format. Outputs html if different from `roff`.\n  Defaults to `roff`.\n\n* name\n  The name shown in the manpage header, if it isn't given in the ronn header like in this README.\n  Defaults to empty string.\n\n* section\n  The section shown in the manpage header, if it isn't given in the ronn header like in this README.\n  Defaults to empty string.\n\n* version\n  The version shown in the manpage header.\n  Defaults to empty string.\n\n* manual\n  The MANUAL string shown in the manpage header.\n  Defaults to empty string.\n\n* date\n  The date shown in the manpage header.\n  Defaults to now, must be acceptable by new Date(string).\n\n`marked-man` invokes `marked --gfm --sanitize`.\nThe --breaks option can be helpful to match default `ronn` behavior.\n\n\nSEE ALSO\n--------\n\n[Ryan Tomayko](https://github.com/rtomayko/ronn)\n\n\nREPORTING BUGS\n--------------\n\nSee [marked-man repository](https://github.com/kapouer/marked-man).\n","maintainers":[{"name":"kapouer","email":"kapouer@melix.org"}],"time":{"modified":"2015-07-18T13:53:07.955Z","created":"2013-05-12T22:46:05.224Z","0.0.1":"2013-05-12T22:46:10.744Z","0.0.2":"2013-05-19T19:22:16.266Z","0.1.0":"2014-01-04T23:24:46.383Z","0.1.1":"2014-01-04T23:37:04.268Z","0.1.2":"2014-05-05T07:47:39.785Z","0.1.3":"2014-05-05T09:10:41.674Z","0.1.4":"2014-09-21T08:40:20.131Z","0.1.5":"2015-07-18T13:53:07.955Z"},"author":{"name":"Jérémy Lal","email":"kapouer@melix.org"},"repository":{"type":"git","url":"git://github.com/kapouer/marked-man.git"},"keywords":["markdown","roff","ronn"],"bugs":{"url":"https://github.com/kapouer/marked-man/issues"},"license":"MIT","readmeFilename":"README.md","users":{"julien-f":true,"kapouer":true},"_attachments":{},"_etag":"\"2XIYA39JX8C3QOLGLGTJ2BTML\""}